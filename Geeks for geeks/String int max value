import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		Scanner scan = new Scanner(system.in);
		int num = scan.nextInt();
		for(int i = 0; i < num; i++){
		    String toCheck = scan.nextLine();
		    System.out.println(maxVal(toCheck));
		}
	}
	
	public static int maxVal(String s){
	    int currentResult = 0;
	    int currentDigit = 0;
	    for(int i = 0; i < s.length(); i++){
	        currentDigit = digitConverter(s.getChar(i));
	        if(currentResult == 0 || currentResult == 1) currentReuslt += currentDigit;
	        else if(currentDigit == 1) currentResult += currenDigit;
	        else if(currentDigit > 1) currentResult *= currentDigit;
	    }
	    
	    return currentResult;
	}
	
	public static int digitConverter(char s){
	    return s - '0';
	}
}
